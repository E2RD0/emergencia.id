	<parameter name="usuariocreador" class="java.lang.String"/>
	<parameter name="id_perfil_medico" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
	ROW_NUMBER () OVER () AS numero,
	perfil_medico.nombres,
	perfil_medico.apellidos,
	perfil_medico.id_perfil_medico,
	perfil_medicacion.nombre,
	perfil_medicacion.dosis
FROM perfil_medicacion
	INNER JOIN perfil_medico ON 
	 perfil_medicacion.id_perfil_medico = perfil_medico.id_perfil_medico 
	INNER JOIN usuario ON 
	 perfil_medico.id_usuario = usuario.id_usuario 
WHERE 
	 usuario.email =  $P{usuariocreador} 
	 AND perfil_medicacion.id_perfil_medico = $P{id_perfil_medico} 
 GROUP BY perfil_medico.id_perfil_medico,
	perfil_medicacion.nombre,
	perfil_medicacion.dosis]]>
